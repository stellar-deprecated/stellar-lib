{
  "name": "stellar-lib",
  "version": "0.10.0",
  "description": "Stellar JavaScript client library",
  "files": [
    "src/js/*",
    "bin/*",
    "build/*",
    "test/*",
    "Makefile",
    "Gruntfile.js"
  ],
  "main": "src/index",
  "directories": {
    "test": "test"
  },
    "dependencies": {
        "sjcl-codec-bytes": "git+https://github.com/zaach/sjcl-codec-bytes.git#0ddc77444403bbdc176ff0450c2b50100cf81c4c",
        "async": "~0.9.0",
        "extend": "~1.2.1",
        "lru-cache": "~2.5.0",
        "superagent": "^0.18.0",
        "gulp-uglify": "~0.3.0",
        "gulp-rename": "~1.2.0",
        "tweetnacl": "git+https://github.com/stellar/tweetnacl-js.git",
        "sjcl": "~1.0.0",
        "jsbn": "~0.0.0"
    },
    "devDependencies": {
        "mocha": "~1.14.0",
        "gulp": "~3.6.2",
        "gulp-concat": "~2.2.0",
        "gulp-jshint": "~1.5.5",
        "webpack": "~1.1.11",
        "map-stream": "~0.1.0",
        "istanbul": "~0.2.10",
        "coveralls": "~2.10.0",
        "nock": "^0.34.1",
        "gulp-exec": "~2.1.0"
    },
    "scripts": {
        "build": "node_modules/.bin/gulp",
        "test": "./node_modules/.bin/istanbul test -x build/sjcl.js -x src/js/jsbn/* ./node_modules/.bin/_mocha -- --reporter spec test/*-test.js",
        "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
    },
  "repository": {
    "type": "git",
    "url": "git://github.com/stellar/stellar-lib.git"
  },
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=0.10.0"
  }
}
